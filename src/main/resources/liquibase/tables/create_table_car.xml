<?xml version="1.0" encoding="utf-8" ?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="create_table_car" author="liquibase">
        <createTable tableName="car_entity">
            <column name="ID" type="BIGSERIAL">
                <constraints nullable="false" primaryKey="true" primaryKeyName="CAR_PK"/>
            </column>
            <column name="targa" type="VARCHAR(10 BYTE)">
                <constraints nullable="false"/>
            </column>

            <column name="model" type="VARCHAR(30 BYTE)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>
